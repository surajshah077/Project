@model List<Team>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Image</th>
            <th scope="col">Name</th>
            <th scope="col">Degisnation</th>
            <th scope="col">Logo</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td><img src="~/@obj.Image" width="100" height="100" /></td>
                <td>@obj.Name</td>
                <td>@obj.Degisnation</td>
                <td><img src="~/@obj.Logo" width="100" height="100" /></td>
                <td>
                    <a asp-action="Edit" asp-controller="Team" asp-route-id="@obj.TeamId">Edit</a>
                    <a asp-action="Delete" asp-controller="Team" asp-route-id="@obj.TeamId">Delete</a>
                </td>
            </tr>
        }
        <a class="submit" class="btn bnt-primary" asp-action="Create" asp-controller="Team">Create</a>
    </tbody>
</table>
