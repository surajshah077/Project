@model List<About>

<table class="table">
    <thead>
        <tr>
            @*        <th scope="col">Description</th> *@
            <th scope="col">Image</th>
            <th scope="col">JoinDate</th>
            <th scope="col">EndDate</th>
            <th scope="col">Title</th>
            <th scope="col">TDescription</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                @*  <td>@obj.Description </td> *@
                <td><img src="~/@obj.Image" width="100" height="100" /></td>
                <td>
                    <script>
                        var joinDate = new Date('@obj.JoinDate');
                        var month = joinDate.toLocaleString('default', { month: 'long' });
                        var year = joinDate.getFullYear();
                        document.write(month + ' ' + year);
                    </script>
                <td>@obj.EndDate</td>
                <td>@obj.Title</td>
                <td>@obj.TDescription</td>
                <td>
                    <a asp-action="Edit" asp-controller="About" asp-route-id="@obj.AboutId">Edit</a>
                    <a asp-action="Delete" asp-controller="About" asp-route-id="@obj.AboutId">Delete</a>
                </td>
            </tr>
        }
        <a class="submit" class="btn bnt-primary" asp-action="Create" asp-controller="About">Create</a>
       
    </tbody>

</table>
