@model List<Services>
<table class="table">
    <thead>
        <tr>
         @*    <th scope="col">Title</th> *@
            <th scope="col">Topic</th>
            <th scope="col">Description</th>
            <th scope="col">Logo</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
               @*  <td>@obj.Title </td> *@
                <td>@obj.Topic </td>
                <td>@obj.Description</td>
                <td><img src="~/@obj.Logo" width="100" height="100"/></td>

                <td>
                    <a asp-action="Edit" asp-controller="Services" asp-route-id="@obj.ServiceId">Edit</a>
                    <a asp-action="Delete" asp-controller="Services" asp-route-id="@obj.ServiceId">Delete</a>
                </td>
            </tr>
        }
        <a class="submit" class="btn bnt-primary" asp-action="Create" asp-controller="Services">Create</a>
    </tbody>
</table>
